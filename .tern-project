{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "jquery",
    "chai"
  ],
  "loadEagerly": [
    "src/**/*.js"
  ],
  "dontLoad": [
    "coverage/**/*.js",
    "index.js",
    "index.min.js",
    "rollup.config.js"
  ],
  "plugins": {
    "complete_strings": {},
    "node": {},
    "lint": {},
    "angular": {},
    "requirejs": {},
    "modules": {},
    "es_modules": {},
    "doc_comment": {
      "strong": true,
      "fullDocs": true
    }
  }
}
