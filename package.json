{
  "name": "utemplate",
  "version": "2.2.7",
  "description": "Templates are supported by default in HTML5, this is a single lightweight script to prepare them for you.",
  "main": "bin/utemplate.js",
  "scripts": {
    "pretest": "plato -r -d report src -e .eslintrc.json",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "preversion": "rollup -c",
    "postversion": "babel src -d lib",
    "prepublish": "uglifyjs bin/utemplate.js -c --source-map bin/utemplate.min.js.map --source-map-root bin -o bin/utemplate.min.js"
  },
  "eslintConfig": {
    "sourceType": "module",
    "extends": "standard",
    "plugins": [
      "standard",
      "promise"
    ],
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    }
  },
  "repository": {
    "type": "git",
    "url": "github.com/Announcement/utemplate"
  },
  "keywords": [
    "template",
    "html5",
    "shim",
    "javascript",
    "ecma6",
    "small",
    "micro",
    "easy",
    "lightweight",
    "prepare"
  ],
  "author": "Jacob Francis Powers",
  "license": "MIT",
  "dependencies": {
    "helpers": "file:///C:\\Users\\Francis\\Documents\\Projects\\helpers"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^3.9.1",
    "eslint-config-google": "^0.7.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-standard": "^2.0.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.0.0",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-requirejs": "^1.1.0",
    "karma-rollup-plugin": "^0.2.4",
    "mocha": "^3.1.2",
    "plato": "^1.7.0",
    "requirejs": "^2.3.2",
    "rollup": "^0.34.13",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.13.0",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-sourcemaps": "^0.3.7",
    "uglify-js": "^2.7.4"
  }
}
